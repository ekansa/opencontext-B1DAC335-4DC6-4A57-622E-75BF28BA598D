  <arch:observations>
    <arch:observation obsNumber="1">
      <arch:links>
        <arch:docID type="person" info="Principle Author / Analyst">67303C62-0129-48BF-45D3-A15172495D61</arch:docID>
        <oc:person_links>
          <oc:link href="http://opencontext.org/persons/67303C62-0129-48BF-45D3-A15172495D61" cite="contributor">
            <oc:name>Canan Çakırlar</oc:name>
